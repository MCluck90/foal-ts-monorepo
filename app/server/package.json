{
  "private": true,
  "name": "@app/server",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf -rf ./build tsconfig.tsbuildinfo",
    "build": "tsc",
    "rebuild": "yarn run clean && yarn run build",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "test": "jest --watch",
    "test:ci": "jest --watchAll=false --ci",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "prettier": "@config/prettier",
  "_moduleAliases": {
    "~": "./build"
  },
  "dependencies": {
    "@config/prettier": "*",
    "@foal/cli": "^2.5.0",
    "@foal/core": "^2.5.0",
    "@manager/administration": "*",
    "@types/jest": "^26.0.23",
    "jest": "26.6.0",
    "module-alias": "^2.2.2",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "26.5.6",
    "typescript": "^4.3.4"
  }
}
