{
  "compilerOptions": {
    "outDir": "build",
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false,
    "module": "commonjs",
    "target": "es2017",
    "rootDir": "src",
    "lib": ["es2017", "dom"],
    "baseUrl": "src",
    "paths": {
      "~/*": ["./*"],
      "@config/prettier/*": ["../.config/prettier/src/*"],
      "@config/prettier": ["../../.config/prettier/src"],
      "@manager/administration": ["../../manager/administration/src"],
      "@manager/administration/*": ["../../manager/administration/src/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "references": [
    {
      "path": "../../.config/prettier"
    },
    {
      "path": "../../manager/administration"
    }
  ]
}
