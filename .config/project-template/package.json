{
  "private": true,
  "name": "@project/template",
  "version": "1.0.0",
  "exports": {
    ".": "./build/index.js",
    "./": "./build/"
  },
  "scripts": {
    "clean": "rimraf -rf ./build tsconfig.tsbuildinfo",
    "build": "tsc",
    "rebuild": "yarn run clean && yarn run build",
    "watch": "tsc -w",
    "test": "jest",
    "test:all": "yarn test --watchAll=false --ci",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "prettier": "@config/prettier",
  "dependencies": {
    "@config/prettier": "*",
    "@types/jest": "^26.0.23",
    "jest": "26.6.0",
    "rimraf": "^3.0.2",
    "ts-jest": "26.5.6",
    "typescript": "^4.3.4"
  }
}
