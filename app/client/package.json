{
  "name": "@app/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "watch": "cross-env BROWSER=none craco start",
    "eject": "craco eject",
    "clean": "rimraf ./build",
    "build": "craco build",
    "rebuild": "yarn clean && yarn build",
    "lint": "eslint \"src/**\"",
    "test": "craco test",
    "test:ci": "craco test --watchAll=false --ci --passWithNoTests && prettier --write tsconfig.json",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6006 -s public",
    "preinstall": "path-exists ../../scripts/verify-dependency && node ../../scripts/verify-dependency.js || exit 0"
  },
  "prettier": "@config/prettier",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "@config/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "jest": {
    "moduleNameMapper": {
      "^~(.*)": "<rootDir>/src$1"
    }
  },
  "dependencies": {
    "@optiqs/optiqs": "^1.1.1",
    "@optiqs/projections": "^1.4.0",
    "@utility/common": "*",
    "fp-ts": "^2.10.5",
    "monocle-ts": "^2.3.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.6.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "typed-redux-saga": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@config/eslint-config": "*",
    "@config/prettier": "*",
    "@craco/craco": "^6.1.2",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/components": "^6.3.0",
    "@storybook/core-events": "^6.3.0",
    "@storybook/node-logger": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.4",
    "@storybook/theming": "^6.3.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "babel-loader": "^8.0.0",
    "babel-plugin-macros": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": ">=7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.1",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2",
    "react-refresh": ">=0.8.3 <0.10.0",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "ts-loader": "8",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "webpack": "^4"
  }
}
